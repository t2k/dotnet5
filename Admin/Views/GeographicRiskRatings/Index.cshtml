@model IEnumerable<KYC.Entities.GeographicRiskRating>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Country)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RiskClassUS)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RatingDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RiskScore)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Country)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RiskClassUS)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RatingDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RiskScore)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
